{
  "manifest_version": 2,
  "name": "Covid19 Tracker & Reminder",
  "version": "1.0.1",
  "description": "Monitora a covid 19 e lembra-te de lavar as m√£os",
  "permissions": ["alarms", "storage", "tabs"],
  "background": {
    "scripts": ["helper.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
